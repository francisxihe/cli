{
  "name": "template-main-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "vue-tsc",
    "build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
    "prod": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build --mode prod",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "build:no-cache": "pnpm clean:cache && pnpm run build",
    "clean": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "preinstall": "node ../../scripts/preinstall.js",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less,scss} --custom-syntax postcss-html",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less,scss} --custom-syntax postcss-html"
  },
  "dependencies": {
    "@fl/http": "^0.1.6",
    "@wangeditor/editor": "^5.1.21",
    "@wangeditor/editor-for-vue": "^1.0.2",
    "axios": "^0.27.2",
    "element-ui": "^2.15.13",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.21",
    "vue": "2.x",
    "vue-router": "3.x"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@types/fs-extra": "^9.0.13",
    "@types/nprogress": "^0.2.0",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/babel-plugin-transform-vue-jsx": "^1.4.0",
    "api": "workspace:*",
    "chalk": "4.x",
    "components": "workspace:*",
    "directive": "workspace:*",
    "echarts": "workspace:*",
    "fs-extra": "^10.1.0",
    "layout": "workspace:*",
    "magic-string": "^0.26.7",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.4",
    "qiankun": "^2.10.3",
    "request": "workspace:*",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-visualizer": "^5.8.0",
    "sass": "1.32.0",
    "station": "workspace:*",
    "style": "workspace:*",
    "terser": "^5.15.0",
    "type": "workspace:*",
    "typescript": "^4.4.4",
    "utils": "workspace:*",
    "vite": "^3.0.9",
    "vite-plugin-babel": "^1.1.2",
    "vite-plugin-chunk-split": "^0.4.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mkcert": "^1.10.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^3.5.0",
    "vue-tsc": "^1.0.9"
  }
}