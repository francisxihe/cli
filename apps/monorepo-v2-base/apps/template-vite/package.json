{
  "name": "template-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "build:no-cache": "pnpm clean:cache && pnpm run build",
    "clean": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "preinstall": "node ../../scripts/preinstall.js",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less,scss} --custom-syntax postcss-html",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less,scss} --custom-syntax postcss-html"
  },
  "dependencies": {
    "@fl/http": "^0.1.6",
    "axios": "^0.27.2",
    "element-ui": "^2.15.13",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.21",
    "vue": "2.x",
    "vue-router": "3.x"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vitejs/plugin-vue2-jsx": "^1.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-visualizer": "^5.8.0",
    "sass": "^1.54.8",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "terser": "^5.15.0",
    "typescript": "^4.4.4",
    "vite": "^3.0.9",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^3.5.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^1.0.8",
    "layout": "workspace:*",
    "module-dashboard": "workspace:*",
    "module-center": "workspace:*",
    "station": "workspace:*",
    "request": "workspace:*",
    "directive": "workspace:*",
    "style": "workspace:*"
  }
}
